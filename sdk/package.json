{
  "name": "zkenclave-sdk",
  "version": "0.1.7",
  "description": "TypeScript SDK for privacy-preserving vault withdrawals with ZK proofs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "privacy",
    "zk",
    "zero-knowledge",
    "ethereum",
    "vault",
    "withdrawal",
    "private",
    "horizen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zkenclave/sdk"
  },
  "author": "zkenclave-team",
  "dependencies": {
    "ethers": "^6.11.0",
    "poseidon-lite": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "ethers": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
